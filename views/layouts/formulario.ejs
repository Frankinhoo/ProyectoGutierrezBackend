<form id="formProductos" method="POST" action="api/productos">
    <h1>ZAPATILLAS</h1>
    <div>
        <label for="producto">Modelo</label>
        <input id="producto" type="text" name="producto" required>
    </div>
    <div>
        <label for="marca">Marca</label>
        <input id="marca" type="text" name="marca" required>
    </div>
    <div>
        <label for="precio">Precio</label>
        <input id="precio" type="number" name="precio" required>
    </div>
    <div>
        <input type="submit">
    </div>
</form>
